# Порт сервера
PORT=3001

# База данных PostgreSQL
DATABASE_URL="postgresql://username:password@localhost:5432/mkvr_app"

# JWT секрет
JWT_SECRET=your-super-secret-jwt-key-here

# JWT время жизни токена (в секундах)
JWT_EXPIRES_IN=86400

# Режим окружения
NODE_ENV=development

# CORS настройки
CORS_ORIGIN=http://localhost:3000

# Логирование
LOG_LEVEL=info 